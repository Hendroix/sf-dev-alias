alias c='clear'
alias na='nano ~/.zshrc'
alias aarun='sf apex run'
alias aarunf='aarun --file '
alias sfr="sf project retrieve start"
alias sfd="sf project deploy start"
alias sfdv="sf project deploy validate"
alias sfrc="sfr -c"
alias sfrcm="sfrc -m"
alias sfrcd="sfrc -d"
alias sfrcx="sfrc -x"
alias sfdc="sfd -c"
alias sfdq="sf project deploy quick"
alias sfdqi="sfdq --job-id"
alias sfdqr="sfdq --use-most-recent"
alias sfdcd="sfdc -d"
alias sfdcdr="sfdc --dry-run"
alias sfdcm="sfdc -m"
alias sfdcx="sfdc -x"
alias sfcp="sf community publish"
alias gf="git fetch"
